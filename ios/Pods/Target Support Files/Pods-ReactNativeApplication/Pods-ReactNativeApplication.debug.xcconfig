FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Amplitude-iOS" "${PODS_CONFIGURATION_BUILD_DIR}/Analytics" "${PODS_CONFIGURATION_BUILD_DIR}/Segment-Amplitude"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Amplitude-iOS/Amplitude_iOS.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Analytics/Analytics.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Segment-Amplitude/Segment_Amplitude.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Amplitude_iOS" -framework "Analytics" -framework "Segment_Amplitude"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
